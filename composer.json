{
    "require": {
        "cilex/cilex": "~1.0",
        "chobie/jira-api-restclient": "2.0.x-dev",
        "frlnc/php-slack": "~1.0",
        "monolog/monolog": "~1.0",
        "pda/pheanstalk": "~3.0",
        "sebastian/version": "~1.0",
        "symfony/process": "~2.0",
        "tedivm/stash": "0.13.*",
        "yosymfony/toml": "0.3.*",
        "zendesk/zendesk_api_client_php": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Pingu\\": "src/"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pingu\\Test\\": "test"
        }
    },
    "bin": [
        "bin/pingu"
    ],
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-v2": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
